<%- include("./../partials/header"); %>
  <div class="input-container">
    <form method="POST" action="/addItem" enctype="application/x-www-form-urlencoded">
      <h3>Add Items</h3>
      <br />
      <label for="Iname">Name</label>
      <input type="text" id="Iname" name="itemName" placeholder="Item Name......" required /><br /><br />
      <label for="quantity">Quantity</label>
      <input type="number" id="quantity" name="itemQuantity" placeholder="Quantity" required /><br /><br />
      <label for="Price">Price</label>
      <input type="number" id="Price" name="itemPrice" placeholder="Price..." required /><br /><br />
      <label for="ImageUrl">Image Link</label>
      <input type="text" id="ImageUrl" name="itemImageUrl" placeholder="Image Url Link" required /><br /><br />
      <input type="submit" value="Submit" />
    </form>
  </div>
  <%- include("./../partials/footer"); %>